model Users {
  id              String   @id @default(uuid())
  name            String   @unique
  nickname        String   
  email           String
  cpf             String
  profession      String
  specialization  String
  phone           String
  password        String
  city            String
  state           String
  created_at      DateTime @default(now())
  updated_at      DateTime @default(now())
}
